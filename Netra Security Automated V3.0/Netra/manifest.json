{
    "name":"Netra Security",
    "version":"3.0",
    "manifest_version":3,
    "description":"Simple URL/Domain Validator to check if the Wbsite is Safe or Not.",
    "author": "Siddhesh Surve",
    "action":{
        "default_icon":"logo.png"
    },
    "icons":{
        "128":"logo.png"
    },
    "permissions":["activeTab"],
    "content_scripts":[{"js":["script.js"],
                        "matches":["*://*/*"]}],
    "host_permissions":["*://*/*"]
}
